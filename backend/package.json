{
  "name": "challenge2",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "node dist/server.js",
    "build": "sucrase ./src -d ./dist --transforms imports && cp -r ./src/app/views ./dist/app/",
    "dev": "nodemon src/server",
    "queue:dev": "nodemon src/queue.js",
    "queue:prod": "node dist/queue.js",
    "migrate": "sequelize db:migrate",
    "migration:g": "sequelize migration:generate --name",
    "migrate:undo": "sequelize db:migrate:undo",
    "migrate:to": "sequelize db:migrate:undo:all --to",
    "debug": "node --inspect-brk -r sucrase/register src/server.js",
    "fake": "snowboard mock doc/documentation.apib",
    "aglio": "aglio -i doc/documentation.apib --theme-variables default -o doc/index.html"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "bee-queue": "^1.2.2",
    "cors": "^2.8.5",
    "date-fns": "^2.0.1",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "express-brute": "^1.0.1",
    "express-brute-redis": "^0.0.1",
    "express-handlebars": "^3.1.0",
    "express-rate-limit": "^5.0.0",
    "helmet": "^3.21.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.7.5",
    "multer": "^1.4.2",
    "nodemailer": "^6.3.0",
    "nodemailer-express-handlebars": "^3.1.0",
    "pg": "^7.12.1",
    "pg-hstore": "^2.3.3",
    "rate-limit-redis": "^1.6.0",
    "redis": "^2.8.0",
    "sequelize": "^5.15.2",
    "sharp": "^0.23.1",
    "socket.io": "^2.2.0",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "eslint": "^6.3.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.0",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2",
    "sequelize-cli": "^5.5.0",
    "snowboard": "^3.3.2",
    "sucrase": "^3.10.1"
  }
}
